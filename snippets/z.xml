<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="github"
          fetch=".." />

  <remote name="gitlab"
          fetch="https://gitlab.com" />

  <remote name="mld"
          fetch="https://github.com/maldini03"
          revision="ten" />

  <project path="manifest" name="android_manifest" remote="mld" />
  
  <!-- Build -->
  <project path="build/make" name="android_build" remote="mld">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" remote="mld">
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Device -->
  <project path="device/zproject/sepolicy" name="android_device_zproject_sepolicy" remote="mld" />
  <project path="device/qcom/sepolicy" name="android_device_qcom_sepolicy" remote="mld" />

  <!-- External -->
  <project path="external/skia" name="android_external_skia" remote="mld" />
  
  <!-- Frameworks -->
  <project path="frameworks/av" name="android_frameworks_av" remote="mld" />
  <project path="frameworks/base" name="android_frameworks_base-1" remote="mld" />
  <project path="frameworks/native" name="android_frameworks_native" remote="mld" />
  <project path="frameworks/opt/net/ims" name="android_frameworks_opt_net_ims" remote="mld" />
  <project path="frameworks/opt/net/wifi" name="android_frameworks_opt_net_wifi" remote="mld" />
  <project path="frameworks/opt/telephony" name="android_frameworks_opt_telephony" remote="mld" />

  <!-- Hardware -->
  <project path="hardware/zproject/interfaces" name="android_hardware_zproject_interfaces" remote="mld" />
  <project path="hardware/interfaces" name="android_hardware_interfaces" remote="mld" />
  
  <!-- Packages -->
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="mld" />
  
  <!-- Prebuilts -->
  <project path="prebuilts/build-tools" name="android_prebuilts_build-tools" remote="mld" />
  <project path="prebuilts/tools-extras" name="android_prebuilts_tools-extras" remote="mld" />

  <!-- System -->
  <project path="system/core" name="android_system_core" remote="mld" />
  <project path="system/sepolicy" name="android_system_sepolicy" remote="mld" />
  
  <!-- Vendor -->
  <project path="vendor/zproject" name="android_vendor_zproject" remote="mld" />
  <project path="vendor/support" name="android_vendor_support" remote="mld" />

</manifest>